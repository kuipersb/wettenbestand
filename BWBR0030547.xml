<paragraaf><kop><label>Paragraaf</label> <nr status="officieel">3</nr> <titel status="officieel">Uit de markt nemen</titel></kop>

<artikel status="goed"><kop><label>Artikel</label> <nr status="officieel">4</nr></kop>

